<form method="post" action="<%= students_url %>">
  <input 
    type="hidden" 
    name="authenticity_token" 
    value="<%= form_authenticity_token %>">
  
  <label>First Name
    <input
      type="text"
      name="student[first_name]"
      value="<%= student.first_name %>">
  </label>
  
  <label>Last Name
    <input 
      type="text"
      name="student[last_name]"
      value="<%= student.last_name %>">
  </label>
  
  <label>Email
    <input
      type="text"
      name="student[email]"
      value="<%= student.email %>">
  </label>
  
  <label>Cohort</label>
  
  <button>Save</button>
  
</form>